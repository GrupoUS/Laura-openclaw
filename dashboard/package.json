{
  "name": "dashboard",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run --hot src/server/index.ts & vite",
    "snapshot": "bun run scripts/snapshot-openclaw.ts",
    "build": "bun run snapshot && vite build && bun build src/server/index.ts --outdir dist --target bun && cp -r src/server/data dist/data",
    "type-check": "tsc --noEmit",
    "lint:check": "oxlint --config .oxlintrc.json src/",
    "lint:fix": "oxlint --config .oxlintrc.json --fix src/",
    "test": "vitest run",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.2.0",
    "@tanstack/router-plugin": "^1.161.3",
    "@types/bun": "latest",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.1.4",
    "bun-types": "^1.3.9",
    "drizzle-kit": "^0.31.9",
    "oxlint": "^1.49.0",
    "tailwindcss": "^4.2.0",
    "vite": "7"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.78.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@google/genai": "^1.42.0",
    "@hono/node-server": "^1.19.9",
    "@hono/trpc-server": "^0.4.2",
    "@neondatabase/serverless": "^1.0.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-query": "^5.90.21",
    "@tanstack/react-router": "^1.161.3",
    "@trpc/client": "11",
    "@trpc/react-query": "11",
    "@trpc/server": "11",
    "@upstash/redis": "^1.36.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.45.1",
    "hono": "^4.12.0",
    "immer": "^11.1.4",
    "ioredis": "^5.9.3",
    "iron-session": "^8.0.4",
    "lucide-react": "^0.575.0",
    "next-themes": "^0.4.6",
    "react": "19",
    "react-dom": "19",
    "recharts": "^3.7.0",
    "tailwind-merge": "^3.5.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  }
