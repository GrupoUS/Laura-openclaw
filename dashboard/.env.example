# Neon PostgreSQL Connection URL
DATABASE_URL="postgres://user:pass@ep-rest-of-host.region.aws.neon.tech/neondb?sslmode=require"

# OpenClaw Gateway Security Token
GATEWAY_TOKEN="your_secure_random_string_here"

# WebSocket URL for the OpenClaw gateway
# Local development: ws://127.0.0.1:3333 (or ws://127.0.0.1:18789)
# Railway production: Use a tunnel URL (e.g. Cloudflare Tunnel, ngrok, Tailscale)
#   because Railway cannot reach localhost on your machine!
GATEWAY_WS_URL="ws://127.0.0.1:18789"

# Secret for OpenClaw agent REST API authentication (x-laura-secret header)
LAURA_API_SECRET="generate-with-openssl-rand-base64-32"

# (Optional) Gateway call timeout in milliseconds (default: 15000)
# GATEWAY_TIMEOUT_MS=15000

