{
  "name": "openclaw-admin",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run --hot src/server/index.ts & vite",
    "build": "vite build && bun build src/server/index.ts --outdir dist --target bun",
    "type-check": "tsc --noEmit",
    "lint:check": "oxlint --config .oxlintrc.json src/",
    "lint:fix": "oxlint --config .oxlintrc.json --fix src/",
    "test": "vitest run",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate"
  },
  "devDependencies": {
    "@tanstack/router-plugin": "^1.161.3",
    "@types/bun": "latest",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.1.4",
    "bun-types": "^1.3.9",
    "drizzle-kit": "^0.31.9",
    "oxlint": "^1.49.0",
    "vite": "7"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "@hono/trpc-server": "^0.4.2",
    "@neondatabase/serverless": "^1.0.2",
    "@tanstack/react-query": "^5.90.21",
    "@tanstack/react-router": "^1.161.3",
    "@trpc/client": "11",
    "@trpc/react-query": "11",
    "@trpc/server": "11",
    "drizzle-orm": "^0.45.1",
    "hono": "^4.12.0",
    "react": "19",
    "react-dom": "19",
    "zod": "^4.3.6"
  }
}
